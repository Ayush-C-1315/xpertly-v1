{
  "expo": {
    "name": "xpertly-v1",
    "slug": "xpertly-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/images/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2E"
    },
    "plugins": [
      "expo-router"
   ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
